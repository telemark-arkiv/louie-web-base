  },
  "devDependencies": {
    "coveralls": "^2.11.4",
    "standard": "^5.4.1",
    "tap": "^2.3.1"
  },
  "dependencies": {
    "handlebars": "^4.0.5",
    "hapi": "^11.1.2",
    "hapi-auth-cookie": "^3.1.0",
    "hoek": "^3.0.4",
    "inert": "^3.2.0",
    "jsonwebtoken": "^5.4.1",
